                <div class="collapse" id="dashboard-collapse" style="">
                    <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                        <li>
                            <a class="link-dark rounded btn" asp-controller="Employee" asp-action="Index">View</a>
                        </li>
                        <li>
                            <a class="link-dark rounded btn" asp-controller="Employee" asp-action="Add">Add New</a>
                        </li>
                    </ul>
                </div>
            </li>
            @*<li class="mb-1">
                <button class="btn btn-toggle align-items-center rounded collapsed" data-bs-toggle="collapse" data-bs-target="#orders-collapse" aria-expanded="false">
                    Country
                </button>
                <div class="collapse" id="orders-collapse" style="">
                    <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                        <li>
                            <a class="link-dark rounded btn" asp-controller="Country" asp-action="Index">View</a>
                        </li>
                        <li>
                            <a class="link-dark rounded btn" asp-controller="Country" asp-action="AddCountry">Add New</a>
                        </li>
                    </ul>
                </div>
            </li>*@
            <li class="border-top my-3"></li>
            @*<li class="mb-1">
                <button class="btn btn-toggle align-items-center rounded collapsed" data-bs-toggle="collapse" data-bs-target="#account-collapse" aria-expanded="false">
                    Customer
                </button>
                <div class="collapse" id="account-collapse" style="">
                    <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                        <li>
                            <a class="link-dark rounded btn">View</a>
                        </li>
                        <li>
                            <a class="link-dark rounded btn">Add New</a>
                        </li>
                    </ul>
                </div>
            </li>*@
        </ul>
    </div>

    <main role="main" class="pb-5 pl-3 pr-3 col">
        @RenderBody()
